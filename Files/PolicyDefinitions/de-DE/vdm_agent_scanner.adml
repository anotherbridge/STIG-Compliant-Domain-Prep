      <string id="Compression_Mode_Disable">Deaktivieren</string>
      <string id="Compression_Mode_Lossless">Verlustfrei</string>
      <string id="Compression_Mode_JPEG">JPEG</string>
      <string id="Compression_JPEGQuality_NumericName">JPEG-Komprimierungsqualität:</string>
